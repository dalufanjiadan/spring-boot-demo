<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.justdoit.demo.mapper.ProductMapper">

	<insert id="insertProduct" useGeneratedKeys="true" keyProperty="id" parameterType="com.justdoit.demo.model.Product">
		INSERT INTO 
			user(name, price)
		VALUES
			(#{name}, #{price})
	</insert>
</mapper>